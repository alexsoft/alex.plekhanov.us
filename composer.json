{
    "name": "alexsoft/alex.plekhanov.us",
    "description": "Alex.Plekhanov.us",
    "keywords": [],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "daylerees/kurenai": "^1.1",
        "erusev/parsedown": "^1.5",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "spatie/laravel-sitemap": "^5.0",
        "stidges/country-flags": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Alex\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
